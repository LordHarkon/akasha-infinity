{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"target": "ESNext",
		"rootDir": "src",
		"outDir": "dist",
		"removeComments": true,
		"strict": false,
		"skipLibCheck": true,
		"noUnusedParameters": false,
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"baseUrl": "./",
		"paths": {
			"#config": ["src/config.ts"],
			"#database/*": ["src/lib/database/*"],
			"#structures/*": ["src/lib/structures/*"],
			"#lib/*": ["src/lib/*"],
			"#typings/*": ["src/typings/*"],
			"#models/*": ["src/models/*"],
			"#assets/*": ["src/assets/*"]
		}
	},
	"include": ["src"]
}
